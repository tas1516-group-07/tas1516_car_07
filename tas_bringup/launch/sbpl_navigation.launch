<launch>
  <param name="/use_sim_time" value="true"/>

  <node pkg="move_base" type="move_base" respawn="false" name="move_base_node" output="screen" >
    <param name="footprint_padding" value="0.01" />
    <param name="controller_frequency" value="10.0" />
    <param name="controller_patience" value="100.0" />
    <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS" />
    <param name="base_global_planner" value="SBPLLatticePlanner" />
    <param name="SBPLLatticePlanner/primitive_filename" value="$(find tas_bringup)param/tas_car.mprim" />
    <rosparam file="$(find tas_bringup)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find tas_bringup)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find tas_bringup)/param/local_costmap_params.yaml" command="load" ns="local_costmap"/>
    <rosparam file="$(find tas_bringup)/param/global_costmap_params.yaml" command="load"  ns="global_costmap"/>
    <rosparam file="$(find tas_bringup)/param/sbpl_global_params.yaml" command="load" />
    <rosparam file="$(find tas_bringup)/param/dwa_planner_params.yaml" command="load" />
  </node>
</launch>
